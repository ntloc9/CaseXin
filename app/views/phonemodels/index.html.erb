<p id="notice"><%= notice %></p>

<h1>Phonemodels</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @phonemodels.each do |phonemodel| %>
      <tr>
        <td><%= phonemodel.title %></td>
        <td><%= link_to 'Show', phonemodel %></td>
        <td><%= link_to 'Edit', edit_phonemodel_path(phonemodel) %></td>
        <td><%= link_to 'Destroy', phonemodel, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Phonemodel', new_phonemodel_path %>
